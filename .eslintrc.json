{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "standard",
        "plugin:jest/all",
        "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": "off"
    }
}

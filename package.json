{
  "name": "data-structures",
  "version": "1.0.0",
  "description": "Data structures for Node & Browser",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "",
    "build": "tsc",
    "lint-staged": "",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.ts\"",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm test && npm run lint",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utils-js/data-structures.git"
  },
  "keywords": [
    "Data Structures",
    "DS",
    "Collections",
    "Typescript"
  ],
  "author": "Sujeet Kumar Jaiswal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/utils-js/data-structures/issues"
  },
  "homepage": "https://github.com/utils-js/data-structures#readme",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "ts-node": "^8.10.2"
  }
}
